  <outfile>../../build/gantw.exe</outfile>
  <priority>normal</priority>
  <downloadUrl>http://www.oracle.com/technetwork/java/javase/downloads/index.html</downloadUrl>
  <icon>../ico/gant.ico</icon>
  <versionInfo>
    <companyName>The Groovy Project</companyName>
    <copyright>Â©2016 The Groovy Project</copyright>
    <productName>Gant</productName>
    <productVersion>1.9.11.0</productVersion>
    <txtProductVersion>1.9.11</txtProductVersion>
    <fileVersion>1.0.0.0</fileVersion>
